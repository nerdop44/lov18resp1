            <field name="name">Numero de control retenciones IVA</field>
            <field name="code">retention.iva.control.number</field>
            <field name="padding">8</field>
        </record>

        <record forcecreate="True" id="sequence_islr_retention" model="ir.sequence">
            <field name="name">Numero de control retenciones ISLR</field>
