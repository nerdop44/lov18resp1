        </record>

        <record forcecreate="True" id="sequence_municipal_retention" model="ir.sequence">
            <field name="name">Numero de control retenciones Municipal</field>
            <field name="code">retention.municipal.control.number</field>
            <field name="padding">5</field>
	</record>
    </data>
</odoo>
