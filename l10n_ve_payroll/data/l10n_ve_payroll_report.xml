<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="l10n_ve_payroll_report_inces" model="account.report">
        <field name="name">Aportes y Retenciones Inces</field>
        <field name="filter_unfold_all" eval="True"/>
        <field name="filter_partner" eval="False"/>
        <field name="filter_show_draft" eval="False"/>
        <field name="filter_unreconciled" eval="False"/>
        <field name="filter_period_comparison" eval="False"/>
        <field name="filter_date_range" eval="True"/>
        <field name="filter_multi_company">selector</field>
        <field name="default_opening_date_filter">this_quarter</field>
        <field name="search_bar" eval="True"/>
        <field name="custom_handler_model_id" ref="model_l10n_ve_payroll_report_handler_inces"/>
    </record>

    <record id="action_l10n_ve_payroll_report_inces" model="ir.actions.client">
        <field name="name">Aportes y Retenciones Inces</field>
        <field name="tag">account_report</field>
        <field name="context" eval="{'report_id': ref('l10n_ve_payroll.l10n_ve_payroll_report_inces')}" />
    </record>

    <record id="l10n_ve_payroll_report_faov" model="account.report">
        <field name="name">Aportes y Retenciones FAOV</field>
        <field name="filter_unfold_all" eval="True"/>
