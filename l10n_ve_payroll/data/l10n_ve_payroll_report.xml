        <field name="search_bar" eval="True"/>
        <field name="custom_handler_model_id" ref="model_l10n_ve_payroll_report_handler_faov"/>
        <field name="column_ids">
            <record id="payroll_report_faov_sal_integral" model="account.report.column">
                <field name="name">Salario Integral</field>
                <field name="expression_label">sal_integral</field>
                <field name="figure_type">monetary</field>
            </record>
            <record id="payroll_report_faov_ret_faov" model="account.report.column">
                <field name="name">Retenido</field>
                <field name="expression_label">ret_faov</field>
                <field name="figure_type">monetary</field>
            </record>
            <record id="payroll_report_faov_aporte_faov" model="account.report.column">
                <field name="name">Aporte</field>
                <field name="expression_label">aporte_faov</field>
                <field name="figure_type">monetary</field>
            </record>
            <record id="payroll_report_faov_total" model="account.report.column">
                <field name="name">Total</field>
                <field name="expression_label">total</field>
                <field name="figure_type">monetary</field>
            </record>
        </field>
    </record>

